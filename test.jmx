            <stringProp name="script">var jsonData = JSON.parse(prev.getResponseDataAsString());
vars.put(&quot;availableCodesCount&quot;, jsonData.meta.available);
var codes = [];
var availablesCount = 0;
for (var i = 0; i &lt; jsonData.meta.total; i++) {
    var codeObject = jsonData.data[i];
    if (codeObject.isUsed === false) {
        vars.put(&quot;code&quot;, codeObject.code);
        codes[availablesCount] = codeObject.code;
        availablesCount++;
    }
}
vars.put(&quot;codes&quot;, JSON.stringify(codes));

